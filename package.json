{
  "name": "@blackhuman/recogito-js",
  "version": "1.4.1",
  "description": "A JavaScript library for text annotation with the optional headless mode",
  "main": "dist/recogito.min.js",
  "publishConfig": {
    "@blackhuman:registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "start": "webpack serve --open --mode=development",
    "build": "webpack --mode=production",
    "build-dev": "webpack --mode=development",
    "build-polyfills": "webpack --mode=production --config webpack.polyfills.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blackhuman/recogito-js.git"
  },
  "keywords": [
    "RecogitoJS",
    "Annotation"
  ],
  "author": "August White",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/blackhuman/recogito-js/issues"
  },
  "homepage": "https://github.com/blackhuman/recogito-js#readme",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties"
      ]
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.6",
    "html-webpack-plugin": "^4.5.2",
    "mini-css-extract-plugin": "^1.6.0",
    "sass": "^1.35.2",
    "sass-loader": "^10.2.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@recogito/recogito-client-core": "1.3.4",
    "axios": "^0.21.1",
    "preact": "^10.5.13",
    "tiny-emitter": "^2.1.0"
  }
}
